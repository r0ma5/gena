900000000421ca84 9000000004620000 9000000004564970 [ 0.000000]
90000000046f7d78 9000000002cc9f70 90000000002c1918 900000000470e000 [
0.000000] 9000000004564970 90000000040bc0e0 90000000046f7d78
0000000000000000 [ 0.000000] 0000000000004000 90000000045ccd00
0000000000000000 90000000002c1918 [ 0.000000] 90000000002c1900
900000000470b700 9000000004b4df78 9000000004620000 [ 0.000000]
90000000046200a8 90000000046200a8 0000000000000000 9000000004218b2c [
0.000000] 9000000004270008 0000000000000001 0000000000000000
90000000045ccd00 [ 0.000000] ... [ 0.000000] Call Trace: [ 0.000000]
[<90000000040e3f28>] reserve_bootmem_region+0xfc/0x21c [ 0.000000]
[<900000000421ca84>] memblock_free_all+0x114/0x350 [ 0.000000]
[<9000000004218b2c>] mm_core_init+0x138/0x3cc [ 0.000000]
[<9000000004200e38>] start_kernel+0x488/0x7a4 [ 0.000000]
[<90000000040df0d8>] kernel_entry+0xd8/0xdc [ 0.000000] [ 0.000000]
